{
  "name": "github-status-api-gui",
  "version": "0.5.0",
  "description": "GUI tool to interact with GitHub Status API",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src start-config-server lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild": "npm run clean-dist && npm run lint && npm run test",
    "build": "babel-node tools/build.js && npm run open:dist",
    "test": "node --harmony_proxies node_modules/jest/bin/jest",
    "test:cover": "npm run test -- --coverage ",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/lcov-report/index.html",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "start-config-server": "node ./configServer/configServerRunner.js"
  },
  "author": "Michael Hadjiosif",
  "license": "MIT",
  "dependencies": {
    "axis.js": "1.2.1",
    "bootstrap": "3.3.7",
    "config": "^1.25.1",
    "es6-error": "^4.0.2",
    "font-awesome": "^4.7.0",
    "jquery": "~3.2.1",
    "moment": "^2.17.1",
    "object-assign": "^4.1.1",
    "octonode": "^0.7.11",
    "react": "15.3.0",
    "react-dom": "15.3.0",
    "react-fontawesome": "^1.6.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-router-redux": "~4.0.7",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "toastr": "2.1.2",
    "url-search-params": "^0.7.0"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "6.18.0",
    "babel-core": "6.20.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.16.0",
    "browser-sync": "~2.18.5",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "~2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "mockdate": "^2.0.1",
    "node-sass": "^4.5.2",
    "npm-run-all": "^3.1.2",
    "open": "0.0.5",
    "postcss-loader": "^1.2.1",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.3.0",
    "redux-immutable-state-invariant": "^1.2.4",
    "replace": "0.3.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^4.1.1",
    "style-loader": "0.13.1",
    "url-loader": "^0.5.8",
    "webpack": "~2.2.1",
    "webpack-bundle-analyzer": "~2.1.1",
    "webpack-dev-middleware": "~1.9.0",
    "webpack-hot-middleware": "~2.13.2",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/compumike08/GitHub_Status_API_GUI"
  }
}
